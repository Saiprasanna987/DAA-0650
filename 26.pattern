#include <stdio.h>
int main() {
    int n, i, j, k;
    printf("D.sai prasanna kumar,192210609\n");
    printf("Enter the number of levels: ");
    scanf("%d", &n);
    for (i = 1; i <= n; i++) {
        for (j = 1; j <= n - i; j++) {
            printf("   ");
        }
        for (k = 1; k <= i; k++) {
            printf("%4d ", k);
        }
        printf("\n");
    }
    return 0;
}
