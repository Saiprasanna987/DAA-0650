#include <stdio.h>
int main() {
    int n, i;
    printf("D.sai prasanna kumar,192210609\n");
    int min, max;
    printf("Enter the number of elements in the list: ");
    scanf("%d", &n);
    if (n <= 0) {
        printf("The list must contain at least one element.\n");
        return 1;
    }
    int arr[n];
    printf("Enter the elements of the list:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    min = max = arr[0];
    for (i = 1; i < n; i++) {
        if (arr[i] < min) {
            min = arr[i];
        }
        if (arr[i] > max) {
            max = arr[i];
        }
	}
    printf("Minimum value: %d\n", min);
    printf("Maximum value: %d\n", max);
    return 0;
}
