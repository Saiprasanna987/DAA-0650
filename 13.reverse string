#include <stdio.h>
#include<string.h>
int main() {
	printf("D.sai prasanna kumar,192210609\n");
    char str[100];
    int i, j;
    printf("enter the string:\n");
    scanf("%s",&str);
    for (i = 0, j = strlen(str) - 1; i < j; i++, j--) {
        char temp = str[i];
        str[i] = str[j];
        str[j] = temp;
    }
    printf("Reversed string: %s\n", str);
    return 0;
}
