#include<stdio.h>
int main()
{
	printf("D.sai prasanna kumar,192210609\n");
	int i,n,sum=0;
	printf("enter a number:");
	scanf("%d",&n);
	for(i=1;i<n;i++)
	{
		if(n%i==0)
		{
			sum+=i;
		}
	}
	if(sum==n)
	{
		printf("%d is perfect number",n);
	}
	else
	{
		printf("%d is not perfect number",n);
	}
	
}
